{
	//https://tools.knowledgewalls.com/jsontostring
	"hostSettings": [
		{
			"IsUsingWindowsAuthentication": true,
			"IsUsingJwtAuthentication": true,
			"HostBaseUri": "MyHostName",
			"HostNameAlias": "MyHostAlias",
			"Endpoints": [
				{
					"Method": "get",
					"Path": "PathToAppendToHostBaseUri",
					"UriAlias": "MyAlias1",
					"Payload": "{\"someJson\" : \"ToString\"}"
				},
				{
					"Method": "post",
					"Path": "PathToAppendToHostBaseUri",
					"UriAlias": "MyAlias1"
				}
			]
		}
	]
}